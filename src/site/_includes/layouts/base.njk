<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="/images/favicon.ico">
    <link rel="stylesheet" href="/css/main.css" />
    <title>{{ title }}</title>
  </head>
  <body class="{{ pageClass }}" >
    <header>
      <div class="container">
      {% block header %}
      <h1>JAMstack project templates</h1>
      {% include "navigation.njk" %}
      {% endblock %}
      </div>
    </header>
    {# <div class="search-panel">
      <div class="container">
        <input type="search" />
      </div>
    </div> #}
    <div class="container">
      <h2>{{ title }}</h2>
      {{ content | safe }}
    </div>
    <footer>
      {% include "footer.njk" %}
    </footer>
  </body>
</html>



